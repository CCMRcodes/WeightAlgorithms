	number = {S2},
	urldate = {2019-12-06},
	journal = {Journal of General Internal Medicine},
	author = {Maguen, Shira and Madden, Erin and Cohen, Beth and Bertenthal, Daniel and Neylan, Thomas and Talbot, Lisa and Grunfeld, Carl and Seal, Karen},
