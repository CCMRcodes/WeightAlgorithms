	journal = {Pharmacoepidemiology and Drug Safety},
	author = {Huizinga, Mary Margaret and Roumie, Christianne L. and Greevy, Robert A. and Liu, Xulei and Murff, Harvey J. and Hung, Adriana M. and Grijalva, Carlos G. and Griffin, Marie R.},
	month = nov,
