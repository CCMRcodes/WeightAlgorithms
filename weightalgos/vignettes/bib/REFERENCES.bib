	journal = {Journal of General Internal Medicine},
	author = {Breland, Jessica Y. and Phibbs, Ciaran S. and Hoggatt, Katherine J. and Washington, Donna L. and Lee, Jimmy and Haskell, Sally and Uchendu, Uchenna S. and Saechao, Fay S. and Zephyrin, Laurie C. and Frayne, Susan M.},
	month = apr,
	year = {2017},
	pages = {11--17},
	file = {Breland et al. - 2017 - The Obesity Epidemic in the Veterans Health Admini.pdf:C\:\\Users\\VHAANNEvansRR\\Zotero\\storage\\W2HZRMB5\\Breland et al. - 2017 - The Obesity Epidemic in the Veterans Health Admini.pdf:application/pdf}
