	journal = {General Hospital Psychiatry},
	author = {Littman, Alyson J. and Damschroder, Laura J. and Verchinina, Lilia and Lai, Zongshan and Kim, Hyungjin Myra and Hoerster, Katherine D. and Klingaman, Elizabeth A. and Goldberg, Richard W. and Owen, Richard R. and Goodrich, David E.},
